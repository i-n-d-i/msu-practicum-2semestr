%include "io.inc"
;дано двухбайтовое натуральное число n. определить, является ли оно степенью двойки
section .text
global CMAIN
CMAIN:
    GET_UDEC 2, ax        ;get n
    mov bx, 0
D:  shr ax, 1             ;цикл - делим, пока делится  ax = ax / 2
    jz B                  ;if(ax == 0)    goto B
    adc bx, 0             
    jmp D
B:  adc bx, 0 
    cmp bx, 1
    je print              ;if(bx == 1)   goto print
    PRINT_STRING 'NO'
    jmp finish
print:
    PRINT_STRING 'YES'
finish:
    xor eax, eax
    ret