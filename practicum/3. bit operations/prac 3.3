%include "io.inc"
;дано двухбайтовое натуральное число n. вывести логарифм по основанию 2 от n
section .text
global CMAIN
CMAIN:
    GET_UDEC 2, ax      ;get n
    mov bx, 0           ;счетчик
K:  shr ax, 1           ;ax = ax / 2
    jz finish
    inc bx              ;bx++
    jmp K
finish:
    PRINT_UDEC 2, bx 
    xor eax, eax
    ret